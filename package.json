{
  "name": "babel-plugin-ivi-jsx",
  "version": "1.0.0",
  "description": "Babel plugin to compile ivi jsx",
  "main": "dist/index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:marvinhagemeister/babel-plugin-ivi-jsx.git"
  },
  "author": "Marvin Hagemeister <marvin@marvinhagemeister.de>",
  "license": "MIT",
  "scripts": {
    "cleanup": "rimraf dist/",
    "lint": "tslint 'src/**/*.ts' 'src/**/*.tsx'",
    "watch": "tsc -w",
    "build": "npm run cleanup && tsc",
    "test": "mocha --watch-extensions txt --compilers ts:ts-node/register 'src/**/__tests__/*.spec.ts'",
    "test-watch": "npm t -- -w -R min",
    "coverage": "nyc npm t",
    "prepublishOnly": "npm run lint && npm t && npm run build",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@types/babel-generator": "^6.25.0",
    "@types/node": "^8.0.14",
    "babel-core": "^6.25.0",
    "babel-generator": "^6.25.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-traverse": "^6.25.0",
    "babel-types": "^6.25.0",
    "babylon": "^6.17.4",
    "ivi": "^0.8.0",
    "ivi-core": "^0.1.0",
    "ivi-dom": "^0.1.0",
    "ivi-events": "^0.1.0",
    "ivi-html": "^0.1.0",
    "ivi-scheduler": "^0.1.0"
  },
  "devDependencies": {
    "@types/babel-core": "^6.7.15",
    "@types/babel-types": "^6.25.1",
    "@types/chai": "^4.0.1",
    "@types/mocha": "^2.2.41",
    "chai": "^4.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.2",
    "mocha": "^3.3.0",
    "nyc": "^11.0.3",
    "prettier": "^1.5.3",
    "rimraf": "^2.6.1",
    "ts-node": "^3.2.1",
    "tslint": "^5.5.0",
    "tslint-config-sevenval": "^0.6.3",
    "typescript": "^2.4.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --trailing-comma=all --write",
      "npm run lint",
      "git add"
    ]
  },
  "nyc": {
    "include": ["src/*.ts", "src/**/*.ts"],
    "exclude": [
      "typings",
      "node_modules/",
      "**/__tests__/**",
      "__tests__/**",
      "**/*.d.ts"
    ],
    "extension": [".ts"],
    "require": ["ts-node/register"],
    "reporter": ["json", "html", "text", "text-summary", "lcovonly"],
    "all": true
  }
}
